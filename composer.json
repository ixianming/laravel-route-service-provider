{
    "name": "ixianming/laravel-route-service-provider",
    "description": "Laravel's route management extension, supports Laravel 5.3 and above, Laravel 6, Laravel 7, Laravel 8. Based on Laravel's route service provider, provides more convenient and powerful routing management services. Please read README.MD for more details.",
    "keywords": [
        "laravel",
        "laravel5",
        "laravel6",
        "laravel7",
        "laravel8",
        "route",
        "router",
        "routing",
        "route service provider",
        "routing management",
        "routing file management"
    ],
    "homepage": "https://github.com/ixianming/laravel-route-service-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ixianming",
            "email": "ixianming@sina.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^5.3|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ixianming\\Routing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ixianming\\Routing\\RouteServiceProvider"
            ]
        }
    }
}