{
    "name": "ixianming/laravel-route-service-provider",
    "description": "This extension package is based on Laravel's route service provider. This extension package provides richer and more powerful routing management functions. For more details, please read README.MD.",
    "keywords": [
        "laravel",
        "laravel5",
        "laravel6",
        "laravel7",
        "route",
        "router",
        "routing",
        "route service provider",
        "routing management",
        "routing file management"
    ],
    "homepage": "https://github.com/ixianming/laravel-route-service-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ixianming",
            "email": "ixianming@sina.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "Ixianming\\Routing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ixianming\\Routing\\RouteServiceProvider"
            ]
        }
    }
}